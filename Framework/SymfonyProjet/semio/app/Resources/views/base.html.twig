<!doctype html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="author" content="Cédric">
    <!-- Bootstrap -->
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    {% block stylesheets %}
{#
        <link href="{{ asset('bundles/lddtmain/css/bootstrap.css') }}" rel="stylesheet">
#}
    {% endblock stylesheets %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <style>
        body {
            padding-top: 60px; /* 60px to make the container go all the way to the bottom of the topbar */
        }
    </style>
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
<!--
    <script src="../assets/js/html5shiv.js"></script>
-->
    <!-- [endif] -->
    <title>{% block title %}Semio, centre de conférences{% endblock title %}</title>
</head>
<body>
{% block body %}{% endblock %}
<script src="https://code.jquery.com/jquery.js"></script>
{% block javascript %}
<script type="text/javascript" src="{{ asset('bundles/lddtmain/js/bootstrap.js') }}"></script>
{% endblock javascript %}
</body>
</html>